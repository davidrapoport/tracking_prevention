{
	"name": "Cookie Monster",
	"version": "1.0",
	"description": "Deletes old cookies from sites you haven't visited recently",
	"permissions": ["activeTab", "declarativeContent", "storage", 
		"cookies", "http://*/*", "https://*/*", "history","alarms"],
	"background": {
		"scripts": ["delete_cookies.js", "background.js"],
		"persistent": true
	},
	"options_page": "options.html",
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "imgs/shield16.png",
			"32": "imgs/shield32.png",
			"64": "imgs/shield64.png",
			"128": "imgs/shield128.png"
		}
	},
	"icons": {
		"16": "imgs/shield16.png",
		"32": "imgs/shield32.png",
		"64": "imgs/shield64.png",
		"128": "imgs/shield128.png"
	},
	"manifest_version": 2
}
